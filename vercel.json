{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-src https://*.bale.ai;"
        },
        {
          "key": "X-Frame-Options",
          "value": ""
        }
      ]
    }
  ]
}
